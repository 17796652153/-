page {
  background-color: #f5f5f5;
}
.headerTabs {
  width: 750rpx;
  height: 88rpx;
  background: #ffffff;
  justify-content: space-around;
}
.headerTabs .tabsItem {
  flex: 1;
  color: #999;
  font-size: 36rpx;
  text-align: center;
  line-height: 88rpx;
  position: relative;
}
.headerTabs .activeTabs {
  color: #FF2D2D;
}
.headerTabs .activeTabs::after {
  content: "";
  position: absolute;
  width: 64rpx;
  height: 8rpx;
  background: #ff2d2d;
  border-radius: 4rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  bottom: 8rpx;
}
.couponContent {
  padding: 20rpx 30rpx;
}
.couponContent .receiveCoupon {
  width: 690rpx;
  height: 100rpx;
  background: #ffffff;
  padding: 16rpx 20rpx;
  margin-bottom: 20rpx;
}
.couponContent .receiveCoupon .receive image {
  width: 68rpx;
  height: 68rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}
.couponContent .receiveCoupon .receive text {
  color: #333;
  font-size: 28rpx;
  vertical-align: middle;
}
.couponContent .receiveCoupon .more text {
  color: #999;
  font-size: 24rpx;
  margin-right: 12rpx;
  vertical-align: middle;
}
.couponContent .receiveCoupon .more image {
  width: 24rpx;
  height: 24rpx;
  vertical-align: middle;
}
.couponItem {
  margin-bottom: 20rpx;
  justify-content: space-around;
  position: relative;
  width: 690rpx;
  height: 156rpx;
  background: #ffffff;
  border-radius: 20rpx;
  padding: 0 32rpx;
}
.couponItem::before {
  content: "";
  position: absolute;
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  background-color: #F5F5F5;
  top: 50%;
  left: -32rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.couponItem::after {
  content: "";
  position: absolute;
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  background-color: #F5F5F5;
  top: 50%;
  right: -32rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.couponItem .couponPrice {
  flex-shrink: 0;
  color: #FFCB14;
  margin-right: 20rpx;
  max-width: 110rpx;
}
.couponItem .couponPrice .price {
  font-size: 28rpx;
}
.couponItem .couponPrice .price text {
  font-size: 68rpx;
}
.couponItem .couponPrice .range {
  font-size: 28rpx;
  margin-top: 10rpx;
  -webkit-transform: scale(0.8);
          transform: scale(0.8);
}
.couponItem .used {
  color: #ccc;
}
.couponItem .couponInfo {
  width: 310rpx;
}
.couponItem .couponInfo .couponName {
  display: flex;
  align-items: center;
  margin-bottom: 24rpx;
}
.couponItem .couponInfo .couponName image {
  width: 64rpx;
  height: 64rpx;
  overflow: hidden;
  border-radius: 8rpx;
  margin-right: 20rpx;
}
.couponItem .couponInfo .couponName text {
  width: 230rpx;
  font-size: 28rpx;
  color: #333;
  height: 76rpx;
}
.couponItem .couponInfo .couponName .used {
  color: #ccc;
}
.couponItem .couponInfo .couponTimer {
  width: 370rpx;
  color: #FF2D2D;
  font-size: 28rpx;
  -webkit-transform: scale(0.8);
          transform: scale(0.8);
}
.couponItem .couponInfo .used {
  color: #ccc;
}
.couponItem .couponBtn {
  padding: 8rpx 12rpx;
  line-height: 44rpx;
  background: #ff2d2d;
  border-radius: 8rpx;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  margin-left: 20rpx;
}
.couponItem .unCoupon {
  background-color: #CCCCCC;
  margin-left: 62rpx;
}

