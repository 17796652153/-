page {
  background-color: #f5f5f5;
}
.headerNav {
  width: 750rpx;
  height: 72rpx;
  position: relative;
  z-index: 99;
  overflow: hidden;
  background-color: #fff;
}
.headerNav .scrollView {
  width: 750rpx;
  height: 72rpx;
  display: flex;
  padding-right: 72rpx;
  white-space: nowrap;
}
.headerNav .scrollView view {
  flex-shrink: 0;
}
.headerNav .scrollView .headerNavItem {
  padding: 20rpx;
  min-width: 56rpx;
  font-size: 28rpx;
  color: #333;
  display: inline-block;
}
.headerNav .scrollView .headerNavItem:last-child {
  padding-right: 72rpx;
}
.headerNav .scrollView .activeHeadItem {
  color: #FF2D2D;
}
.headerNav .scrollView .activeHeadItem text {
  position: relative;
}
.headerNav .scrollView .activeHeadItem text::after {
  content: "";
  position: absolute;
  width: 44rpx;
  height: 4rpx;
  background: #ff2d2d;
  border-radius: 2rpx;
  display: inline-block;
  left: 50%;
  bottom: -8rpx;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.headerNav .headerNavImg {
  width: 60rpx;
  height: 72rpx;
  background: #ffffff;
  box-shadow: 2rpx 0rpx 12rpx 4rpx rgba(0, 0, 0, 0.41);
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  right: 0;
  top: 0;
}
.headerNav .headerNavImg image {
  width: 28rpx;
  height: 28rpx;
}
.allNav {
  width: 100%;
  height: calc(100% - 72rpx);
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 99;
  background-color: #fff;
}
.allNav scroll-view {
  width: 100%;
  height: 100%;
  padding: 20rpx 0;
}
.allNav scroll-view .allNavList {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.allNav scroll-view .allNavList .allNavItem {
  flex-direction: column;
  justify-content: center;
  width: 20%;
  height: 132rpx;
}
.allNav scroll-view .allNavList .allNavItem image {
  width: 72rpx;
  height: 72rpx;
  margin-bottom: 12rpx;
}
.allNav scroll-view .allNavList .allNavItem text {
  color: #333;
  font-size: 24rpx;
}
swiper {
  height: 352rpx;
}
swiper .swiper-item {
  width: 100%;
  height: 352rpx;
}
.couponList {
  padding: 20rpx 30rpx;
}
.couponList .couponItem {
  width: 690rpx;
  height: 306rpx;
  border-radius: 10rpx;
  background-color: #fff;
  position: relative;
  margin-bottom: 20rpx;
}
.couponList .couponItem::before {
  content: "";
  position: absolute;
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  background-color: #F5F5F5;
  right: 184rpx;
  top: -32rpx;
}
.couponList .couponItem::after {
  content: "";
  position: absolute;
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  background-color: #F5F5F5;
  right: 184rpx;
  bottom: -32rpx;
}
.couponList .couponItem .couponLeft {
  position: absolute;
  left: 0;
  top: 0;
  width: 476rpx;
  height: 306rpx;
  padding: 20rpx;
}
.couponList .couponItem .couponLeft .couponInfo {
  display: flex;
  align-items: center;
}
.couponList .couponItem .couponLeft .couponInfo .couponImg {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.couponList .couponItem .couponLeft .couponInfo .couponContent .couponName {
  color: #333;
  font-size: 28rpx;
  width: 210rpx;
}
.couponList .couponItem .couponLeft .couponInfo .couponContent .advantage {
  color: #999;
  font-size: 24rpx;
  margin: 16rpx 0 32rpx;
}
.couponList .couponItem .couponLeft .couponInfo .couponContent .originalPrice {
  font-size: 28rpx;
  color: #333;
}
.couponList .couponItem .couponLeft .couponInfo .couponContent .presentPrice {
  color: #FF2D2D;
  font-size: 28rpx;
}
.couponList .couponItem .couponLeft .couponInfo .couponContent .presentPrice view {
  font-weight: 600;
  display: inline-block;
}
.couponList .couponItem .couponLeft .couponInfo .couponContent .presentPrice view text {
  font-size: 32rpx;
}
.couponList .couponItem .couponLeft .progress-box {
  margin-top: 20rpx;
}
.couponList .couponItem .couponLeft .progress-box progress {
  font-size: 24rpx;
  text-indent: 1em;
}
.couponList .couponItem .couponRight {
  position: absolute;
  right: 0;
  top: 0;
  width: 212rpx;
  height: 306rpx;
  padding: 50rpx 36rpx;
}
.couponList .couponItem .couponRight .couponPrice {
  color: #FF2D2D;
  font-size: 28rpx;
}
.couponList .couponItem .couponRight .couponPrice text {
  font-size: 58rpx;
}
.couponList .couponItem .couponRight .couponStatus {
  font-size: 28rpx;
  color: #FF2D2D;
  margin: 14rpx 0 26rpx;
  text-align: center;
}
.couponList .couponItem .couponRight .couponEdit {
  width: 144rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  background: #ff2d2d;
  border-radius: 8rpx;
  font-size: 28rpx;
  color: #fff;
}

