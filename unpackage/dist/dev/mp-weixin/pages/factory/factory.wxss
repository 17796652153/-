.root {
  position: fixed;
  top: 0;
  height: 100%;
  overflow: hidden;
}
.header {
  width: 100%;
  height: 104rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 30rpx;
}
.header .search {
  width: 100%;
  height: 64rpx;
  background: #ffffff;
  border: 2rpx solid #ff2d2d;
  border-radius: 34rpx;
  display: flex;
  align-items: center;
}
.header .search image {
  width: 40rpx;
  height: 40rpx;
  margin: 0 20rpx;
}
.header .search text {
  color: #ccc;
  font-size: 28rpx;
}
.header .settled {
  width: 184rpx;
  height: 64rpx;
  background: linear-gradient(70deg, #ff8d4d 0%, #ee2b00 100%);
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  flex-shrink: 0;
  margin-left: 20rpx;
}
.header .settled text {
  font-size: 28rpx;
  color: #fff;
  margin: 0 12rpx 0 20rpx;
}
.header .settled image {
  width: 28rpx;
  height: 28rpx;
}
.headerNav {
  width: 750rpx;
  height: 132rpx;
  position: relative;
  overflow: hidden;
}
.headerNav .scrollView {
  width: 750rpx;
  height: 132rpx;
  display: flex;
  padding-right: 72rpx;
  white-space: nowrap;
}
.headerNav .scrollView view {
  flex-shrink: 0;
}
.headerNav .scrollView .headerNavItem {
  min-width: 136rpx;
  font-size: 24rpx;
  color: #333;
  display: flex;
}
.headerNav .scrollView .headerNavItem .headerType {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 20rpx;
  min-width: 136rpx;
}
.headerNav .scrollView .headerNavItem .headerType .typeImg {
  width: 72rpx;
  height: 72rpx;
  overflow: hidden;
  border-radius: 50%;
  position: relative;
}
.headerNav .scrollView .headerNavItem .headerType .typeImg image {
  width: 56rpx;
  height: 56rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translateX(-50%) translateY(-50%);
          transform: translateX(-50%) translateY(-50%);
  z-index: 1;
  border-radius: 50%;
}
.headerNav .scrollView .headerNavItem .headerType text {
  font-size: 24rpx;
  color: #333;
}
.headerNav .scrollView .headerNavItem .activeHeader .typeImg::after {
  content: "";
  position: absolute;
  left: 0;
  height: 0;
  width: 72rpx;
  height: 72rpx;
  background: linear-gradient(180deg, rgba(255, 160, 160, 0.15), #ffa0a0 100%);
}
.headerNav .scrollView .headerNavItem .activeHeader text {
  color: #FF2D2D;
}
.headerNav .scrollView .headerNavItem:last-child {
  padding-right: 72rpx;
}
.headerNav .scrollView .activeHeadItem {
  color: #FF2D2D;
}
.headerNav .scrollView .activeHeadItem text {
  position: relative;
}
.headerNav .scrollView .activeHeadItem text::after {
  content: "";
  position: absolute;
  width: 44rpx;
  height: 4rpx;
  background: #ff2d2d;
  border-radius: 2rpx;
  display: inline-block;
  left: 50%;
  bottom: -8rpx;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.headerNav .headerNavImg {
  width: 50rpx;
  height: 132rpx;
  background: #ffffff;
  box-shadow: 2rpx 0rpx 12rpx 4rpx rgba(0, 0, 0, 0.41);
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 2;
  text-align: center;
  -webkit-writing-mode: vertical-rl;
          writing-mode: vertical-rl;
}
.allNav {
  width: 100%;
  height: calc(100% - 236rpx);
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 99;
  background-color: #fff;
}
.allNav scroll-view {
  width: 100%;
  height: 100%;
  padding: 20rpx 0;
}
.allNav scroll-view .allNavList {
  padding: 0 30rpx;
}
.allNav scroll-view .allNavList .factoryTitle {
  font-size: 28rpx;
  color: #000;
  margin: 20rpx 0;
}
.allNav scroll-view .allNavList .factoryContent {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.allNav scroll-view .allNavList .factoryContent .factoryItem {
  flex-direction: column;
  justify-content: center;
  width: 138rpx;
  height: 132rpx;
}
.allNav scroll-view .allNavList .factoryContent .factoryItem image {
  width: 72rpx;
  height: 72rpx;
  margin-bottom: 12rpx;
  border-radius: 50%;
}
.allNav scroll-view .allNavList .factoryContent .factoryItem text {
  color: #333;
  font-size: 24rpx;
}
.screen {
  margin-top: 20rpx;
  display: flex;
  align-items: center;
}
.screen .screenItem {
  flex: 1;
  height: 68rpx;
  text-align: center;
  line-height: 68rpx;
  font-size: 28rpx;
  color: #999;
  position: relative;
  margin-bottom: 20rpx;
}
.screen .screenItem .activeScreen {
  color: #FF2D2D;
}
.screen .screenItem image {
  width: 28rpx;
  height: 28rpx;
  margin-left: 20rpx;
  vertical-align: middle;
}
.hideScreenBox {
  position: absolute;
  top: 68rpx;
  z-index: 99;
  overflow: hidden;
  transition: all 0.3s;
}
.hideScreenBox .hideScreenContent {
  width: 750rpx;
  height: 84rpx;
  background: #f5f5f5;
  color: #333;
  font-size: 28rpx;
  text-align: center;
  line-height: 84rpx;
}
.hideScreenBox .activeHideContent {
  background-color: #fff;
}
.slotBox {
  height: 0;
  left: -500rpx;
}
.showSoltScreen {
  height: 426rpx;
}
.addrBox {
  height: 0;
  width: 750rpx;
  left: -250rpx;
  display: flex;
}
.cityAggregate {
  height: 588rpx;
  overflow: hidden;
}
.cityAggregate view {
  padding: 28rpx 0;
  line-height: 40rpx;
}
.provinceScroll {
  flex: 2;
  background-color: #F5F5F5;
}
.provinceScroll .activeProvince {
  background-color: #fff;
}
.cityScroll {
  flex: 8;
  background-color: #fff;
}
.cityScroll view {
  text-align: left;
  padding-left: 30rpx;
}
.showAddrScreen {
  height: 588rpx;
}
.goods {
  padding: 0 30rpx 0;
  border-bottom: 4rpx solid #EBEBEB;
}
.shopGoods {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}
.shopGoods .goodsImg {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.shopGoods .goodsInfo {
  width: 418rpx;
}
.shopGoods .goodsInfo .goodsName {
  width: 420rpx;
  height: 70rpx;
  font-size: 28rpx;
  color: #333333;
}
.shopGoods .goodsInfo .goodsSpec {
  height: 44rpx;
  line-height: 44rpx;
  background: #f5f5f5;
  border-radius: 4rpx;
  color: #999;
  font-size: 28rpx;
  position: relative;
  padding-right: 42rpx;
  padding-left: 12rpx;
  margin-top: 12rpx;
}
.shopGoods .goodsInfo .goodsSpec text {
  margin-right: 20rpx;
}
.shopGoods .goodsInfo .goodsSpec image {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  right: 18rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.shopGoods .goodsInfo .goodsOperation {
  margin-top: 32rpx;
}
.shopGoods .goodsInfo .goodsOperation .goodsPrice {
  font-size: 20rpx;
  color: #FF2D2D;
}
.shopGoods .goodsInfo .goodsOperation .goodsPrice text {
  font-size: 32rpx;
}
.shopGoods .manufactor {
  width: 460rpx;
}
.shopGoods .manufactor .manufactorName {
  width: 420rpx;
  font-size: 36rpx;
  color: #333333;
}
.shopGoods .manufactor .range {
  color: #28C50F;
  font-size: 28rpx;
  margin: 10rpx 0 26rpx;
}
.shopGoods .manufactor .advantage {
  color: #333;
  font-size: 28rpx;
}
.shopGoods .manufactor .advantage text {
  margin-right: 20rpx;
}
.shopGoods .manufactor .goodsAddr {
  color: #999;
  font-size: 28rpx;
  max-height: 84rpx;
}
.shopGoods .manufactor .goodsAddr view {
  max-width: 460rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}
.shopGoods .manufactor .goodsAddr image {
  width: 28rpx;
  height: 28rpx;
  vertical-align: middle;
}

