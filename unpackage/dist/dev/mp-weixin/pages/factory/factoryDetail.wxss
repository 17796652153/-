page {
  background-color: #f5f5f5;
}
.headerImg {
  padding: 20rpx 0 0 30rpx;
}
.headerImg .swiperView {
  width: 100%;
}
.headerImg .swiperView swiper-item {
  width: 520rpx;
  height: 300rpx;
}
.headerImg .swiperView swiper-item .swiper-item {
  width: 520rpx;
  height: 300rpx;
}
.headerImg .swiperView swiper-item .swiper-item image {
  border-radius: 20rpx;
  overflow: hidden;
  display: block;
}
.main {
  padding: 20rpx 30rpx;
}
.main .factoryBox {
  width: 690rpx;
  background: #ffffff;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
}
.factoryInfo {
  padding: 20rpx;
}
.factoryInfo .factoryName {
  font-size: 36rpx;
  color: #333;
}
.factoryInfo .business {
  font-size: 28rpx;
  color: #333;
  margin: 20rpx 0 40rpx;
}
.factoryInfo .factoryAddr {
  align-items: flex-start;
}
.factoryInfo .factoryAddr .factoryAddressTxt {
  font-size: 28rpx;
  color: #333;
  max-height: 80rpx;
  max-width: 482rpx;
}
.factoryInfo .factoryAddr .factoryOptions {
  display: flex;
  align-items: center;
}
.factoryInfo .factoryAddr .factoryOptions .factoryIcon {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 20rpx;
}
.factoryInfo .factoryAddr .factoryOptions .factoryIcon image {
  width: 40rpx;
  height: 40rpx;
}
.factoryInfo .factoryAddr .factoryOptions .factoryIcon text {
  color: #333;
  font-size: 26rpx;
  margin-top: 8rpx;
}
.factoryInfo .factoryInfoBtn {
  display: flex;
  align-items: center;
  margin-top: 44rpx;
}
.factoryInfo .factoryInfoBtn .operationInfo {
  width: 190rpx;
  height: 64rpx;
  background: linear-gradient(71deg, #ff8d4d 0%, #ee2b00 100%);
  border-radius: 10rpx;
  text-align: center;
  line-height: 64rpx;
  font-size: 32rpx;
  color: #fff;
  margin-right: 40rpx;
  position: relative;
}
.factoryInfo .factoryInfoBtn .operationInfo:last-child {
  margin-right: 0;
}
.factoryInfo .factoryInfoBtn .operationInfo button {
  position: absolute;
  left: 0;
  top: 0;
  height: 64rpx;
  width: 190rpx;
  opacity: 0;
}
.factorySample {
  padding-bottom: 20rpx;
}
.factorySample .sampleNav {
  display: flex;
  align-items: center;
}
.factorySample .sampleNav .sampleItem {
  flex: 1;
  font-size: 28rpx;
  color: #333;
  text-align: center;
  padding: 20rpx 0 12rpx;
  position: relative;
}
.factorySample .sampleNav .activeSample::after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  width: 32rpx;
  height: 4rpx;
  background: #ff2d2d;
  border-radius: 2rpx;
}
.factorySample .scrollView {
  display: flex;
  margin-top: 28rpx;
  height: 240rpx;
}
.factorySample .scrollView .sampleImg {
  width: 240rpx;
  height: 240rpx;
  border-radius: 20rpx;
  overflow: hidden;
  flex-shrink: 0;
  margin-right: 20rpx;
}
.factorySample .scrollView .sampleImg:last-child {
  margin-right: 0;
}
.factoryArchives {
  padding: 16rpx 20rpx 20rpx;
}
.factoryArchives .title {
  font-size: 36rpx;
  color: #333;
  margin-bottom: 36rpx;
}
.factoryArchives .companyInfo {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.factoryArchives .companyInfo .companyItem {
  width: 33%;
  margin-bottom: 40rpx;
}
.factoryArchives .companyInfo .companyItem .companyTitle {
  font-size: 24rpx;
  color: #999;
  margin-bottom: 12rpx;
}
.factoryArchives .companyInfo .companyItem .companyContent {
  color: 28px;
  font-size: #333;
}
.factoryArchives .qualifications {
  display: flex;
  align-items: center;
}
.factoryArchives .qualifications view {
  flex: 1;
  font-size: 24rpx;
}
.factoryArchives .qualifications .qualTitle {
  color: #999;
}
.factoryArchives .qualifications .qualContent {
  color: #333;
}
.factoryIntroduce {
  padding: 16rpx 54rpx 20rpx 20rpx;
}
.factoryIntroduce .title {
  font-size: 36rpx;
  color: #333;
  margin-bottom: 36rpx;
}
.factoryIntroduce .introduceContent {
  font-size: 28rpx;
  color: #333;
  line-height: 50rpx;
}
.factoryRecommend .title {
  padding-top: 16rpx;
  padding-left: 20rpx;
  padding-bottom: 36rpx;
  font-size: 36rpx;
  color: #333;
}
.goods {
  padding: 0 30rpx 0;
  border-bottom: 4rpx solid #EBEBEB;
}
.shopGoods {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}
.shopGoods .goodsImg {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.shopGoods .goodsInfo {
  width: 418rpx;
}
.shopGoods .goodsInfo .goodsName {
  width: 420rpx;
  height: 70rpx;
  font-size: 28rpx;
  color: #333333;
}
.shopGoods .goodsInfo .goodsSpec {
  height: 44rpx;
  line-height: 44rpx;
  background: #f5f5f5;
  border-radius: 4rpx;
  color: #999;
  font-size: 28rpx;
  position: relative;
  padding-right: 42rpx;
  padding-left: 12rpx;
  margin-top: 12rpx;
}
.shopGoods .goodsInfo .goodsSpec text {
  margin-right: 20rpx;
}
.shopGoods .goodsInfo .goodsSpec image {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  right: 18rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.shopGoods .goodsInfo .goodsOperation {
  margin-top: 32rpx;
}
.shopGoods .goodsInfo .goodsOperation .goodsPrice {
  font-size: 20rpx;
  color: #FF2D2D;
}
.shopGoods .goodsInfo .goodsOperation .goodsPrice text {
  font-size: 32rpx;
}
.shopGoods .manufactor {
  width: 460rpx;
}
.shopGoods .manufactor .manufactorName {
  width: 420rpx;
  font-size: 36rpx;
  color: #333333;
}
.shopGoods .manufactor .range {
  color: #28C50F;
  font-size: 28rpx;
  margin: 10rpx 0 26rpx;
}
.shopGoods .manufactor .advantage {
  color: #333;
  font-size: 28rpx;
}
.shopGoods .manufactor .advantage text {
  margin-right: 20rpx;
}
.shopGoods .manufactor .goodsAddr {
  color: #999;
  font-size: 28rpx;
}
.shopGoods .manufactor .goodsAddr text {
  margin-right: 20rpx;
  vertical-align: middle;
}
.shopGoods .manufactor .goodsAddr .singleHide {
  max-width: 180rpx;
}
.shopGoods .manufactor .goodsAddr image {
  width: 28rpx;
  height: 28rpx;
  vertical-align: middle;
}

