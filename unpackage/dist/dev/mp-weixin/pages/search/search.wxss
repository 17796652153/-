.searchHeader {
  padding: 20rpx 30rpx;
}
.searchHeader .search {
  width: 540rpx;
  height: 64rpx;
  background: #ffffff;
  border: 2rpx solid #ff2d2d;
  border-radius: 34rpx;
  position: relative;
  overflow: hidden;
}
.searchHeader .search image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 20rpx;
  top: 12rpx;
}
.searchHeader .search input {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  padding-left: 80rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
}
.searchHeader .searchBtn {
  width: 120rpx;
  height: 64rpx;
  background: linear-gradient(61deg, #ff8d4d 0%, #ee2b00 100%);
  border-radius: 10rpx;
  font-size: 28rpx;
  color: #fff;
  line-height: 64rpx;
  text-align: center;
}
.historyHeader {
  padding: 20rpx 50rpx 20rpx 30rpx;
}
.historyHeader .historyTitle {
  font-size: 28rpx;
  color: #333;
}
.historyHeader .empty {
  font-size: 28rpx;
  color: #999;
}
.historyList {
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.historyList .historyItem {
  font-size: 24rpx;
  color: #999;
  padding: 8rpx 16rpx;
  background-color: #F5F5F5;
  border-radius: 8rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  display: inline-block;
}

