<view><view class="searchHeader baseflex"><view class="search"><image src="../../static/icon_search-red.png" mode></image><input type="text" placeholder="输入商品名称" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['__set_model',['','searchGoods','$event',[]]]]]]}}" value="{{searchGoods}}" bindconfirm="__e" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="searchBtn" bindtap="__e">搜索</view></view><view class="history"><block wx:if="{{history.length>0}}"><view class="historyHeader baseflex"><view class="historyTitle">历史搜索</view><view data-event-opts="{{[['tap',[['emptyHistory',['$event']]]]]}}" class="empty" bindtap="__e">清空</view></view></block><view class="historyList"><block wx:for="{{history}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['touchstart',[['bindTouchStart',['$event']]]],['touchend',[['bindTouchEnd',['$event']]]],['tap',[['bindTap',['$0',index],[[['history','',index]]]]]],['longtap',[['bingLongTap',[index]]]]]}}" class="historyItem" bindtouchstart="__e" bindtouchend="__e" bindtap="__e" bindlongtap="__e">{{''+item+''}}</view></block></view></view><show-model vue-id="50cad900-1" showModel="{{showModelSingle}}" title="确认删除该历史记录" data-event-opts="{{[['^cancel',[['cancel']]],['^confirm',[['confirmSingle']]]]}}" bind:cancel="__e" bind:confirm="__e" bind:__l="__l"></show-model><show-model vue-id="50cad900-2" showModel="{{showModelAll}}" title="确认删除全部历史记录" data-event-opts="{{[['^cancel',[['cancel']]],['^confirm',[['confirmAll']]]]}}" bind:cancel="__e" bind:confirm="__e" bind:__l="__l"></show-model></view>