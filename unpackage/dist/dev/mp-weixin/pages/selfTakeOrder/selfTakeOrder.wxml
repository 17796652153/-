<view><view class="headNavigation"><view data-event-opts="{{[['tap',[['changeNav',[0]]]]]}}" class="{{[activeNav==0?'navItem activeNav':'navItem']}}" bindtap="__e">待提货</view><view data-event-opts="{{[['tap',[['changeNav',[1]]]]]}}" class="{{[activeNav==1?'navItem activeNav':'navItem']}}" bindtap="__e">已提货</view></view><block wx:if="{{slefTakeList.length>0}}"><block><block wx:for="{{slefTakeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="selfTakeMain"><view class="shopName">{{''+item.store_name+''}}</view><block wx:for="{{item.goods}}" wx:for-item="val" wx:for-index="idx" wx:key="idx"><view class="orderGoods"><view class="goodsImg"><image class="pic" src="{{www+val.goods_icon}}" mode></image></view><view class="goodsInfo"><view class="goodsName multiHide">{{''+val.goods_name+''}}</view><view class="goodsSpec singleHide"><text>{{val.goods_spec_title}}</text></view><view class="goodsOperation baseflex"><view class="goodsPrice">￥<text>{{val.goods_price}}</text></view><block wx:if="{{activeNav==0}}"><view data-event-opts="{{[['tap',[['seePickUpCode',['$0'],[[['slefTakeList','',index,'order_no']]]]]]]}}" class="operationBtn" bindtap="__e">提货码</view></block><block wx:else><view class="operationBtn over">已提货</view></block></view></view></view></block></view></block></block></block><block wx:else><view class="goodsNull">暂无自提订单</view></block></view>