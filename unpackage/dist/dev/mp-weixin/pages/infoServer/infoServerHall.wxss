.root {
  position: fixed;
  top: 0;
  height: 100%;
  overflow: hidden;
}
.headerNav {
  width: 750rpx;
  height: 72rpx;
  position: relative;
  overflow: hidden;
}
.headerNav .scrollView {
  width: 750rpx;
  height: 72rpx;
  display: flex;
  padding-right: 72rpx;
  white-space: nowrap;
}
.headerNav .scrollView view {
  flex-shrink: 0;
}
.headerNav .scrollView .headerNavItem {
  padding: 20rpx;
  min-width: 56rpx;
  font-size: 28rpx;
  color: #333;
  display: inline-block;
}
.headerNav .scrollView .headerNavItem:last-child {
  padding-right: 72rpx;
}
.headerNav .scrollView .activeHeadItem {
  color: #FF2D2D;
}
.headerNav .scrollView .activeHeadItem text {
  position: relative;
}
.headerNav .scrollView .activeHeadItem text::after {
  content: "";
  position: absolute;
  width: 44rpx;
  height: 4rpx;
  background: #ff2d2d;
  border-radius: 2rpx;
  display: inline-block;
  left: 50%;
  bottom: -8rpx;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.headerNav .headerNavImg {
  width: 60rpx;
  height: 72rpx;
  background: #ffffff;
  box-shadow: 2rpx 0rpx 12rpx 4rpx rgba(0, 0, 0, 0.41);
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  right: 0;
  top: 0;
}
.headerNav .headerNavImg image {
  width: 28rpx;
  height: 28rpx;
}
.allNav {
  width: 100%;
  height: calc(100% - 176rpx);
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 99;
  background-color: #fff;
}
.allNav scroll-view {
  width: 100%;
  height: 100%;
  padding: 20rpx 0;
}
.allNav scroll-view .allNavList {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.allNav scroll-view .allNavList .allNavItem {
  flex-direction: column;
  justify-content: center;
  width: 20%;
  height: 132rpx;
}
.allNav scroll-view .allNavList .allNavItem image {
  width: 72rpx;
  height: 72rpx;
  margin-bottom: 12rpx;
}
.allNav scroll-view .allNavList .allNavItem text {
  color: #333;
  font-size: 24rpx;
}
swiper {
  height: 352rpx;
}
swiper .swiper-item {
  width: 100%;
  height: 352rpx;
}
.screen {
  margin-top: 20rpx;
  display: flex;
  align-items: center;
}
.screen .screenItem {
  flex: 1;
  height: 68rpx;
  text-align: center;
  line-height: 68rpx;
  font-size: 28rpx;
  color: #999;
  position: relative;
  margin-bottom: 20rpx;
}
.screen .screenItem .activeScreen {
  color: #FF2D2D;
}
.screen .screenItem image {
  width: 28rpx;
  height: 28rpx;
  margin-left: 20rpx;
  vertical-align: middle;
}
.hideScreenBox {
  position: absolute;
  top: 68rpx;
  z-index: 98;
  overflow: hidden;
  transition: all 0.3s;
}
.hideScreenBox .hideScreenContent {
  width: 750rpx;
  height: 84rpx;
  background: #f5f5f5;
  color: #333;
  font-size: 28rpx;
  text-align: center;
  line-height: 84rpx;
}
.hideScreenBox .activeHideContent {
  background-color: #fff;
}
.slotBox {
  height: 0;
  left: -500rpx;
}
.showSoltScreen {
  height: 252rpx;
}
.addrBox {
  height: 0;
  width: 750rpx;
  left: -250rpx;
  display: flex;
}
.cityAggregate {
  height: 588rpx;
  overflow: hidden;
}
.cityAggregate view {
  padding: 28rpx 0;
  line-height: 40rpx;
}
.provinceScroll {
  flex: 2;
  background-color: #F5F5F5;
}
.provinceScroll .activeProvince {
  background-color: #fff;
}
.cityScroll {
  flex: 8;
  background-color: #fff;
}
.cityScroll view {
  text-align: left;
  padding-left: 30rpx;
}
.showAddrScreen {
  height: 588rpx;
}
.wantBuyMain {
  padding: 20rpx 30rpx;
}
.wantBuy {
  width: 690rpx;
  background: #ffffff;
  border-radius: 20rpx;
  box-shadow: 0rpx 0rpx 12rpx 0rpx rgba(0, 0, 0, 0.1);
  padding: 20rpx;
  margin-bottom: 20rpx;
}
.wantBuy .wantBuyImg {
  width: 300rpx;
  height: 300rpx;
  flex-shrink: 0;
  border-radius: 20rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.wantBuy .wantBuyImg swiper-item {
  width: 100%;
  height: 100%;
}
.wantBuy .wantBuyImg swiper-item .swiper-item {
  width: 100%;
  height: 100%;
}
.wantBuy .wantBuyInfo {
  flex-shrink: 0;
}
.wantBuy .wantBuyInfo .wantBuyUser {
  display: flex;
  align-items: center;
  margin: 20rpx 0;
}
.wantBuy .wantBuyInfo .wantBuyUser .userImg {
  width: 68rpx;
  height: 68rpx;
  overflow: hidden;
  border-radius: 50%;
  margin-right: 20rpx;
}
.wantBuy .wantBuyInfo .wantBuyUser .userName {
  font-size: 36rpx;
  color: #333;
  max-width: 180rpx;
}
.wantBuy .wantBuyInfo .call {
  width: 32rpx;
  height: 32rpx;
}
.wantBuy .wantBuyInfo .describe {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 30rpx;
  width: 336rpx;
  height: 76rpx;
}
.wantBuy .wantBuyInfo .userAddress {
  display: flex;
}
.wantBuy .wantBuyInfo .userAddress .addrImg {
  width: 28rpx;
  height: 28rpx;
  margin-right: 20rpx;
}
.wantBuy .wantBuyInfo .userAddress .address {
  font-size: 28rpx;
  color: #333;
  width: 280rpx;
  height: 80rpx;
}
.wantBuy .wantBuyOperation {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.wantBuy .wantBuyOperation .del {
  color: #999;
  margin-right: 10rpx;
}
.wantBuy .wantBuyOperation .edit {
  color: #FF2D2D;
}
.repairNull {
  margin: 80rpx auto;
  text-align: center;
}
.repairNull image {
  width: 600rpx;
  height: 600rpx;
}
.repairNull .nullTips {
  font-size: 36rpx;
  color: #999;
  text-align: center;
}
.repairFixed {
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  right: 30rpx;
  bottom: 80rpx;
}

