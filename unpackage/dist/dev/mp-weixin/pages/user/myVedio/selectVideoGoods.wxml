<view><view class="searchHeader baseflex"><view class="search"><image src="../../../static/icon_search-red.png" mode></image><input type="text" placeholder="输入商品名称" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['__set_model',['','searchGoods','$event',[]]]]]]}}" value="{{searchGoods}}" bindconfirm="__e" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="searchBtn" bindtap="__e">搜索</view></view><block wx:if="{{goodsList.length>0}}"><view class="goodsList"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectGoods',[index]]]]]}}" class="goodsItem baseflex" bindtap="__e"><view class="goodsSelect"><block wx:if="{{!item.checked}}"><image class="pic" src="../../../static/icon_unSel.png" mode></image></block><block wx:else><image class="pic" src="../../../static/icon_sel.png" mode></image></block></view><view class="goodsContent baseflex"><view class="goodsImg"><image class="pic" src="{{www+item.goods_icon}}" mode></image></view><view class="goodsName singleHide">{{''+item.goods_name+''}}</view></view></view></block></view></block><block wx:else><view class="goodsNull">暂无商品</view></block><view data-event-opts="{{[['tap',[['jumpRepair',['$event']]]]]}}" class="confirmBtn" bindtap="__e">确定商品</view></view>