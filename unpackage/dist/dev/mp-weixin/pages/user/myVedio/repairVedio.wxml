<view><view class="refundBox"><view class="refundType flexBox"><view class="title">视频标题</view><view class="content"><input type="text" placeholder="请输入视频标题" data-event-opts="{{[['input',[['__set_model',['','vedioTitle','$event',[]]]]]]}}" value="{{vedioTitle}}" bindinput="__e"/></view></view><view class="refundType flexBox"><view class="title">正文描述</view><view class="content"><input type="text" placeholder="请输入正文描述" data-event-opts="{{[['input',[['__set_model',['','vedioContent','$event',[]]]]]]}}" value="{{vedioContent}}" bindinput="__e"/></view></view><block wx:if="{{false}}"><view class="refundType flexBox"><view class="title">当前位置</view><view data-event-opts="{{[['tap',[['selAddr',['$event']]]]]}}" class="content" bindtap="__e">{{''+(address||'请选择位置')+''}}</view></view></block></view><view class="refundBox"><view class="repairLastItem" style="border-bottom:2rpx solid #EBEBEB;"><view class="repairItemName">上传视频</view><view class="repairSelImg"><block wx:if="{{!src}}"><view data-event-opts="{{[['tap',[['chooseVideo',['$event']]]]]}}" class="selImgItem addImg" bindtap="__e"><image class="add" src="../../../static/upload.png"></image></view></block><block wx:else><view class="selImgItem delImg"><video class="bgImg video" src="{{src}}"></video><image class="del" mode="aspectFit" src="../../../static/delImg.png" data-event-opts="{{[['tap',[['del',['$event']]]]]}}" bindtap="__e"></image></view></block></view><block wx:if="{{progress>0}}"><view class="progress"><progress percent="{{progress}}" show-info="{{true}}" border-radius="20"></progress></view></block></view></view><view class="refundBox"><view data-event-opts="{{[['tap',[['jumpSelectVideo',['$0'],['selGoods.id']]]]]}}" class="refundType flexBox" style="justify-content:space-between;padding-right:30rpx;" bindtap="__e"><view class="title">选择商品</view><image class="arrowRight" src="../../../static/icon_arrow-rightGray.png" mode></image></view></view><block wx:if="{{selGoods.id}}"><view class="refundBox"><view class="refundType alreadySel flexBox"><view class="title"><image src="{{www+selGoods.goods_icon}}" mode></image></view><view data-event-opts="{{[['tap',[['jumpSelectVideo',['$0'],['selGoods.id']]]]]}}" class="content baseflex" bindtap="__e"><text class="singleHide">{{selGoods.goods_name}}</text><image class="arrowRight" src="../../../static/icon_arrow-rightGray.png" mode></image></view></view></view></block><view data-event-opts="{{[['tap',[['jumpRepair',['$event']]]]]}}" class="confirmBtn" bindtap="__e">提交审核</view></view>