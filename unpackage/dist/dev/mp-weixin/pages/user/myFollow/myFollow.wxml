<view class="myFollow"><block wx:if="{{followList.length>0}}"><view class="followList"><block wx:for="{{followList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="followItem baseflex"><view class="followUser"><image src="{{item.head_img}}" mode="aspectFill"></image><text class="singleHide">{{item.nick_name}}</text></view><block wx:if="{{item.is_like==1}}"><view data-event-opts="{{[['tap',[['clickFollow',['$0','$1',index],[[['followList','',index,'id']],[['followList','',index,'is_like']]]]]]]}}" class="followEdit" bindtap="__e">取消关注</view></block><block wx:else><view data-event-opts="{{[['tap',[['clickFollow',['$0','$1',index],[[['followList','',index,'id']],[['followList','',index,'is_like']]]]]]]}}" class="followEdit cancelFollow" bindtap="__e">关注</view></block></view></block></view></block><block wx:else><view class="goodsNull">暂无关注</view></block></view>