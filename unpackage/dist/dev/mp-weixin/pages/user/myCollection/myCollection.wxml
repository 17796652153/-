<view><block wx:if="{{total>0}}"><view class="collNum">收藏<text>{{total}}</text></view></block><block wx:if="{{dataList.length>0}}"><view class="collShop"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="goods" animation="{{item.data.animationLate}}"><view data-event-opts="{{[['touchstart',[['touchstart',['$event']]]],['touchmove',[['touchmove',[index,'$event']]]],['touchend',[['touchend',['$0'],[[['dataList','',index,'data.id']]]]]]]}}" class="goodsContent" catchtouchstart="__e" catchtouchmove="__e" catchtouchend="__e"><view class="shopGoods"><view class="goodsImg"><image class="pic" src="{{www+item.data.goods_icon}}" mode></image></view><view class="goodsInfo"><view class="goodsName multiHide">{{''+item.data.goods_name+''}}</view><view class="goodsOperation baseflex"><view class="goodsPrice">￥<text>{{item.data.goods_price}}</text></view></view></view></view></view><view class="operation"><view data-event-opts="{{[['tap',[['similar',['$0',index],[[['dataList','',index,'data.cate_one']]]]]]]}}" class="operationBox" bindtap="__e">找相似</view><view data-event-opts="{{[['tap',[['delCollection',['$0',index],[[['dataList','',index,'data.id']]]]]]]}}" class="operationBox operationDel" bindtap="__e">删除</view></view></view></block></view></block><block wx:else><view class="goodsNull">暂无收藏商品</view></block></view>