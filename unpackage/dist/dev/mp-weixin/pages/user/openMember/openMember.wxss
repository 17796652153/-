page {
  background-color: #f5f5f5;
}
.headerMember {
  width: 750rpx;
  height: 320rpx;
  background: #333333;
  padding: 30rpx 60rpx;
}
.headerMember .card {
  width: 630rpx;
  height: 260rpx;
  border-radius: 20rpx;
  background: linear-gradient(67deg, #ffffff 2%, #bcbcbc 100%);
  position: relative;
  padding: 20rpx;
}
.headerMember .card .cardInfo {
  display: flex;
  align-items: center;
}
.headerMember .card .cardInfo .userHead {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 20rpx;
}
.headerMember .card .cardInfo .userInfo .userName {
  max-width: 220rpx;
  font-size: 36rpx;
  color: #333;
}
.headerMember .card .cardInfo .userInfo .userIdentity {
  width: 72rpx;
  height: 32rpx;
  background: #333333;
  border-radius: 8rpx;
  text-align: center;
  line-height: 32rpx;
  font-size: 24rpx;
  color: #FFB82D;
  margin-top: 20rpx;
}
.headerMember .card .memberLevel {
  font-size: 28rpx;
  color: #333;
  position: absolute;
  left: 20rpx;
  bottom: 20rpx;
}
.headerMember .card .memberImg {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  width: 166rpx;
  height: 150rpx;
}
.memberMain {
  padding: 20rpx 30rpx;
  height: calc(98rpx + env(safe-area-inset-bottom));
}
.memberMain .memberType {
  width: 100%;
  height: 220rpx;
  margin-bottom: 40rpx;
}
.memberMain .memberType .swiperView {
  width: 100%;
  height: 220rpx;
}
.memberMain .memberType .swiperView swiper-item {
  width: 482rpx;
  height: 220rpx;
}
.memberMain .memberType .swiperView swiper-item .swiper-item {
  width: calc(100% - 40rpx);
  height: 100%;
  background: linear-gradient(65deg, #eaccaa 0%, #e3c6a6 1%, #cfb094 100%);
  border-radius: 20rpx;
  overflow: hidden;
  position: relative;
  transition: all 0.2s;
}
.memberMain .memberType .swiperView swiper-item .swiper-item .cardInfo {
  width: 100%;
  height: 100%;
  padding: 30rpx;
}
.memberMain .memberType .swiperView swiper-item .swiper-item .cardInfo .openPrice {
  font-size: 30rpx;
  color: #333;
}
.memberMain .memberType .swiperView swiper-item .swiper-item .cardInfo .openPrice text {
  font-size: 64rpx;
  color: #000;
}
.memberMain .memberType .swiperView swiper-item .swiper-item .cardInfo .openLevel,
.memberMain .memberType .swiperView swiper-item .swiper-item .cardInfo .openRights {
  font-size: 28rpx;
  color: #333;
}
.memberMain .memberType .swiperView swiper-item .swiper-item .openMemberImg {
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  width: 166rpx;
  height: 150rpx;
}
.memberMain .memberType .swiperView swiper-item .currentCard {
  border: 4rpx solid #FF5555;
}
.memberMain .memberType .swiperView swiper-item .mount {
  background: linear-gradient(65deg, #f5f5f5 0%, #cfb094 100%);
}
.memberMain .memberType .swiperView swiper-item .quarter {
  background: linear-gradient(65deg, #f5f5f5 0%, #949494 100%);
}
.memberMain .memberType .swiperView swiper-item .halfYear {
  background: linear-gradient(65deg, #e5c177 0%, #e3c6a6 1%, #d19d52 100%);
}
.memberMain .memberType .swiperView swiper-item .year {
  background: linear-gradient(65deg, #7ce8f6 0%, #5ed0f4 100%);
}
.memberMain .memberRights {
  width: 690rpx;
  min-height: 360rpx;
  background: #ffffff;
  border-radius: 10rpx;
  box-shadow: 0rpx 0rpx 16rpx 0rpx rgba(0, 0, 0, 0.1);
  margin-bottom: 40rpx;
}
.memberMain .memberRights .rightsTitle {
  font-size: 36rpx;
  color: #333;
  padding: 16rpx 20rpx;
  border-bottom: 2rpx solid #EBEBEB;
}
.memberMain .memberRights .rightsContent {
  padding: 20rpx;
}
.memberMain .memberRights .rightsContent .rightsItem {
  font-size: 28rpx;
  color: #333;
  line-height: 48rpx;
  text-indent: 1em;
}
.memberMain .advertisement {
  width: 690rpx;
  height: 240rpx;
  border: 20rpx;
  overflow: hidden;
  margin-bottom: 40rpx;
}
.memberMain .advertisement image {
  border-radius: 20rpx;
}
.bottomOperation {
  width: 750rpx;
  height: 98rpx;
  background: #ffffff;
  box-shadow: 0rpx 0rpx 8rpx 2rpx rgba(0, 0, 0, 0.25);
  padding: 20rpx 30rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  height: calc(98rpx + env(safe-area-inset-bottom));
  display: flex;
  justify-content: flex-end;
}
.bottomOperation .payMoney {
  font-size: 24rpx;
  color: #666;
  margin-top: 12rpx;
}
.bottomOperation .payMoney text {
  color: #FF2D2D;
  margin-left: 10rpx;
}
.bottomOperation .payMoney .price {
  font-size: 32rpx;
  margin-left: 0;
}
.bottomOperation .payBtn {
  width: 136rpx;
  height: 64rpx;
  background: #ff4646;
  border-radius: 32rpx;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  line-height: 64rpx;
  margin-left: 40rpx;
}

