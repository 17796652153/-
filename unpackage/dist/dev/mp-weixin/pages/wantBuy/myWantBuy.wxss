.mask {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 99;
}
.model {
  position: fixed;
  left: 50%;
  top: 40%;
  z-index: 999;
  -webkit-transform: translateX(-50%) translateY(-40%);
          transform: translateX(-50%) translateY(-40%);
  width: 488rpx;
  background: #ffffff;
  border-radius: 10rpx;
}
.model .title {
  font-size: 32rpx;
  color: #333;
  padding: 20rpx;
}
.model .content {
  margin-left: 20rpx;
  width: 448rpx;
  height: 56rpx;
  background: #f5f5f5;
  border-radius: 8rpx;
  margin-bottom: 35rpx;
  position: relative;
  font-size: 24rpx;
}
.model .content .selPicker {
  display: inline-block;
  height: 100%;
  width: 100%;
  padding: 16rpx 20rpx;
  box-sizing: border-box;
}
.model .content image {
  position: absolute;
  top: 16rpx;
  right: 20rpx;
  width: 24rpx;
  height: 24rpx;
}
.model .operation {
  height: 80rpx;
  width: 100%;
  border-top: 2rpx solid #EBEBEB;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.model .operation view {
  flex: 1;
  font-size: 32rpx;
  text-align: center;
}
.model .operation .cancel {
  color: #999999;
}
.model .operation .confirm {
  color: #FF2D2D;
}

page {
  background-color: #f5f5f5;
}
.headNavigation {
  width: 750rpx;
  height: 92rpx;
  opacity: 1;
  background: #FFEBEB;
  display: flex;
}
.navItem {
  width: 50%;
  text-align: center;
  line-height: 92rpx;
  color: #999;
  font-size: 32rpx;
  position: relative;
}
.activeNav {
  color: #FF2D2D;
}
.activeNav::after {
  content: "";
  width: 32rpx;
  height: 8rpx;
  opacity: 1;
  background: #FF2D2D;
  border-radius: 12rpx;
  position: absolute;
  left: 50%;
  bottom: 8rpx;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.wantBuyMain {
  padding: 20rpx 30rpx 200rpx;
}
.wantBuyMain .wantBuy {
  width: 690rpx;
  background: #ffffff;
  border-radius: 20rpx;
  box-shadow: 0rpx 0rpx 12rpx 0rpx rgba(0, 0, 0, 0.1);
  padding: 20rpx;
  margin-bottom: 20rpx;
}
.wantBuyMain .wantBuy .wantBuyImg {
  width: 300rpx;
  height: 300rpx;
  flex-shrink: 0;
  border-radius: 20rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.wantBuyMain .wantBuy .wantBuyImg swiper-item {
  width: 100%;
  height: 100%;
}
.wantBuyMain .wantBuy .wantBuyImg swiper-item .swiper-item {
  width: 100%;
  height: 100%;
}
.wantBuyMain .wantBuy .wantBuyInfo {
  flex-shrink: 0;
}
.wantBuyMain .wantBuy .wantBuyInfo .wantBuyUser {
  display: flex;
  align-items: center;
  margin: 20rpx 0;
}
.wantBuyMain .wantBuy .wantBuyInfo .wantBuyUser .userImg {
  width: 68rpx;
  height: 68rpx;
  overflow: hidden;
  border-radius: 50%;
  margin-right: 20rpx;
}
.wantBuyMain .wantBuy .wantBuyInfo .wantBuyUser .userName {
  font-size: 36rpx;
  color: #333;
  max-width: 180rpx;
}
.wantBuyMain .wantBuy .wantBuyInfo .call {
  width: 32rpx;
  height: 32rpx;
}
.wantBuyMain .wantBuy .wantBuyInfo .describe {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 30rpx;
  width: 336rpx;
  height: 76rpx;
}
.wantBuyMain .wantBuy .wantBuyInfo .userAddress {
  display: flex;
}
.wantBuyMain .wantBuy .wantBuyInfo .userAddress .addrImg {
  width: 28rpx;
  height: 28rpx;
  margin-right: 20rpx;
}
.wantBuyMain .wantBuy .wantBuyInfo .userAddress .address {
  font-size: 28rpx;
  color: #333;
  width: 280rpx;
  height: 80rpx;
}
.wantBuyMain .wantBuy .wantBuyOperation {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.wantBuyMain .wantBuy .wantBuyOperation .del {
  color: #999;
  margin-right: 10rpx;
}
.wantBuyMain .wantBuy .wantBuyOperation .edit {
  color: #FF2D2D;
}
.repairFixed {
  width: 100rpx;
  height: 100rpx;
