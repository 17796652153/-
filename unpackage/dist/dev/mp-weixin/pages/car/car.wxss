page {
  background-color: #f5f5f5;
}
.mycontent {
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
  height: 100%;
  width: 100%;
  overflow: scroll;
}
.carTop {
  background-color: #fff;
  padding: 20rpx 30rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  width: 100%;
  height: 76rpx;
}
.carTop .buyNum {
  font-size: 28rpx;
  color: #333;
}
.carTop .buyNum text {
  color: #FF0000;
  margin-left: 10rpx;
}
.carTop .topEdit {
  width: 80rpx;
  height: 36rpx;
  line-height: 32rpx;
  text-align: center;
  border: 2rpx solid #ff2d2d;
  border-radius: 8rpx;
  font-size: 24rpx;
  color: #333;
}
.carBottom {
  width: 750rpx;
  height: 104rpx;
  background: #ffffff;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 99;
  padding: 20rpx 30rpx;
}
.carBottom .btnLeft {
  display: flex;
  align-items: center;
}
.carBottom .btnLeft image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}
.carBottom .btnLeft text {
  color: #999;
  font-size: 28rpx;
}
.carBottom .btnRight {
  display: flex;
  align-items: center;
}
.carBottom .btnRight .carPrice {
  font-size: 20rpx;
  color: #666;
  margin-right: 20rpx;
}
.carBottom .btnRight .carPrice text {
  color: #FF2D2D;
}
.carBottom .btnRight .carPrice .carMoney {
  font-size: 32rpx;
}
.carBottom .btnRight .btnPay {
  width: 136rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  background: #ff4646;
  border-radius: 32rpx;
  color: #fff;
  font-size: 28rpx;
}
.carBottom .btnRight .goFollow {
  width: 192rpx;
  height: 64rpx;
  border: 2rpx solid #ff2d2d;
  border-radius: 34rpx;
  color: #ff2d2d;
  font-size: 28rpx;
  text-align: center;
  line-height: 64rpx;
  margin-right: 20rpx;
}
.yuanChecked {
  display: flex;
  align-items: center;
}
.yuanChecked image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}
.yuanChecked text {
  color: #000;
  font-size: 28rpx;
}
.carMain {
  padding: 20rpx 30rpx;
  background-color: #fff;
  padding-top: 76rpx;
  padding-bottom: 104rpx;
}
.carMain .carShop {
  padding: 20rpx 0;
}
.carMain .carShop .shopName {
  margin-bottom: 22rpx;
}
.shopGoods {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}
.shopGoods .goodsImg {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.shopGoods .goodsInfo {
  width: 418rpx;
}
.shopGoods .goodsInfo .goodsName {
  width: 420rpx;
  height: 70rpx;
  font-size: 28rpx;
  color: #333333;
}
.shopGoods .goodsInfo .goodsSpec {
  height: 44rpx;
  line-height: 44rpx;
  background: #f5f5f5;
  border-radius: 4rpx;
  color: #999;
  font-size: 28rpx;
  position: relative;
  padding-right: 42rpx;
  padding-left: 12rpx;
  margin-top: 12rpx;
}
.shopGoods .goodsInfo .goodsSpec text {
  margin-right: 20rpx;
}
.shopGoods .goodsInfo .goodsSpec image {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  right: 18rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.shopGoods .goodsInfo .goodsOperation {
  margin-top: 32rpx;
}
.shopGoods .goodsInfo .goodsOperation .goodsPrice {
  font-size: 20rpx;
  color: #FF2D2D;
}
.shopGoods .goodsInfo .goodsOperation .goodsPrice text {
  font-size: 32rpx;
}
.shopGoods .goodsInfo .goodsOperation .goodsNumOperation {
  justify-content: flex-start;
  position: relative;
}
.shopGoods .goodsInfo .goodsOperation .goodsNumOperation text {
  display: inline-block;
  text-align: center;
  height: 40rpx;
  line-height: 40rpx;
  width: 40rpx;
}
.shopGoods .goodsInfo .goodsOperation .goodsNumOperation .operationBtn {
  width: 40rpx;
  background: #f2f2f2;
  border-radius: 4rpx;
  font-size: 32rpx;
  font-weight: 600;
}
.shopGoods .goodsInfo .goodsOperation .goodsNumOperation .goodsNum {
  min-width: 60rpx;
  max-width: 80rpx;
  text-align: center;
  color: #333;
  font-size: 28rpx;
}
.carNull {
  margin: 28% auto 0;
  text-align: center;
}
.carNull image {
  width: 600rpx;
  height: 480rpx;
}
.carNull text {
  color: #333;
  font-size: 36rpx;
}
.specsExplain {
  width: 100%;
  background: #ffffff;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  padding: 40rpx 30rpx 20rpx;
  box-sizing: border-box;
  padding-bottom: calc(20rpx + env(safe-area-inset-bottom));
}
.specsExplain .specGoodsInfo {
  display: flex;
  align-items: center;
}
.specsExplain .specGoodsInfo .specGoodsImg {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8rpx;
  overflow: hidden;
  margin-right: 20rpx;
  flex-shrink: 0;
}
.specsExplain .specGoodsInfo .specGoodsContent {
  flex-shrink: 0;
  max-width: 400rpx;
}
.specsExplain .specGoodsInfo .specGoodsName {
  color: #333;
  font-size: 32rpx;
}
.specsExplain .specGoodsInfo .specGoodsPrice {
  color: #FF2D2D;
  font-size: 28rpx;
  margin: 14rpx 0;
}
.specsExplain .specGoodsInfo .specGoodsPrice text {
  font-size: 42rpx;
  margin: 0 4rpx;
}
.specsExplain .specGoodsInfo .specGoodsSpecs {
  color: #999;
  font-size: 28rpx;
  height: 76rpx;
}
.specsExplain .goodsSpecAll {
  margin-top: 40rpx;
}
.specsExplain .goodsSpecAll .specTitle {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 20rpx;
}
.specsExplain .goodsSpecAll .selGoodsSpec {
  width: 100%;
  height: auto;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.specsExplain .goodsSpecAll .selGoodsSpec .goodsSpecItem {
  background: #e5e5e5;
  border-radius: 6rpx;
  padding: 12rpx;
  font-size: 28rpx;
  color: #666;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.specsExplain .goodsSpecAll .selGoodsSpec .activeSpecs {
  color: #fff;
  background-color: #FF2D2D;
}
.specsExplain .buyGoodsNum {
  margin: 50rpx 0 40rpx;
}
.specsExplain .buyGoodsNum .changeNum {
  display: flex;
  align-items: center;
}
.specsExplain .buyGoodsNum .numBtn {
  width: 40rpx;
  height: 40rpx;
  background: #f2f2f2;
  border-radius: 4rpx;
  font-size: 32rpx;
  font-weight: 600;
  line-height: 40rpx;
  text-align: center;
}
.specsExplain .buyGoodsNum input {
  height: 40rpx;
  max-width: 84rpx;
  text-align: center;
  font-size: 28rpx;
  color: #333;
}
.specsExplain .buyOperation {
  width: 690rpx;
  height: 72rpx;
  background: #ff2d2d;
  border-radius: 10rpx;
  line-height: 72rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
}
.specsExplain .close {
  position: absolute;
  right: 30rpx;
  top: 20rpx;
  width: 40rpx;
  height: 40rpx;
}
.mask {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 999;
  width: 100%;
  height: 100%;
}

