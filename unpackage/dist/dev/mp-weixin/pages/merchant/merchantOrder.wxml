<view><view class="headerTabs"><view data-event-opts="{{[['tap',[['changeTabs',[0]]]]]}}" class="{{[activeTabs==0?'tabs activeTab':'tabs']}}" bindtap="__e">待付款</view><view data-event-opts="{{[['tap',[['changeTabs',[1]]]]]}}" class="{{[activeTabs==1?'tabs activeTab':'tabs']}}" bindtap="__e">待发货</view><view data-event-opts="{{[['tap',[['changeTabs',[2]]]]]}}" class="{{[activeTabs==2?'tabs activeTab':'tabs']}}" bindtap="__e">待收货</view><view data-event-opts="{{[['tap',[['changeTabs',[3]]]]]}}" class="{{[activeTabs==3?'tabs activeTab':'tabs']}}" bindtap="__e">售后</view><view data-event-opts="{{[['tap',[['changeTabs',[4]]]]]}}" class="{{[activeTabs==4?'tabs activeTab':'tabs']}}" bindtap="__e">待自提</view></view><block wx:if="{{merchantOrderList.length>0}}"><view class="orderList"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['jumpOrderDetail',['$0'],[[['merchantOrderList','',index,'order_no']]]]]]]}}" class="orderGoods" bindtap="__e"><view class="shopName singleHide">{{''+item.$orig.store_name+''}}</view><block wx:for="{{item.$orig.goods}}" wx:for-item="val" wx:for-index="idx" wx:key="idx"><view class="goods"><view class="goodsImg"><image class="pic" src="{{www+val.goods_icon}}" mode></image></view><view class="goodsInfo"><view class="goodsName multiHide">{{''+val.goods_name+''}}</view><view class="goodsSpecs">{{''+val.goods_spec_title+''}}</view></view><view class="goodsPrice">￥<text class="yuan">{{val.goods_price}}</text></view></view></block><view class="distribution baseflex"><view class="disMode">配送方式<text>{{item.$orig.delivery_type==1?'送货上门':'到店自取'}}</text></view><block wx:if="{{item.$orig.delivery_type==1}}"><view class="disModePrice">{{'￥'+item.$orig.delivery_money+''}}</view></block></view><view class="distribution"><view class="disMode">订单备注<text class="multiHide">{{item.$orig.remark||'无'}}</text></view></view><view class="priceDetail"><view class="totalPrice">总价￥<text class="yuan">{{item.$orig.money}}</text></view><view class="discountPrice" style="margin:0 10rpx;">优惠￥<text class="yuan">{{item.$orig.coupon_money}}</text></view><view class="payPrice">需付款￥<text class="yuan">{{(item.m0*10-item.m1*10)/10}}</text></view></view><block wx:if="{{item.$orig.is_pay==1}}"><view class="operation"><view class="operationBtn redBtn">买家待付款</view></view></block><block wx:else><block wx:if="{{item.$orig.delivery_type==2&&item.$orig.status==1}}"><view class="operation"><view class="operationBtn redBtn">买家待提货</view></view></block><block wx:else><block wx:if="{{item.$orig.status==1}}"><view class="operation"><view class="operationBtn redBtn">待发货</view></view></block><block wx:else><block wx:if="{{item.$orig.status==2}}"><view class="operation"><view class="operationBtn redBtn">买家待收货</view></view></block><block wx:else><block wx:if="{{item.$orig.status==3}}"><view class="operation"><block wx:if="{{!item.$orig.refund_status}}"><view class="operationBtn redBtn">买家已收货</view></block><block wx:else><block wx:if="{{item.$orig.refund_status==1}}"><view class="operationBtn redBtn">买家申请退款</view></block><block wx:else><block wx:if="{{item.$orig.refund_status==2}}"><view class="operationBtn redBtn">已同意</view></block><block wx:else><block wx:if="{{item.$orig.refund_status==3}}"><view class="operationBtn">已拒绝退货</view></block><block wx:else><block wx:if="{{item.$orig.refund_status==4}}"><view class="operationBtn">已退款</view></block></block></block></block></block></view></block><block wx:else><block wx:if="{{item.$orig.status==4}}"><view class="operation"><view class="operationBtn redBtn"></view></view></block><block wx:else><block wx:if="{{item.$orig.status==4}}"><view class="operation"><view class="operationBtn">退货退款待审核</view></view></block></block></block></block></block></block></block></view></block></view></block><block wx:else><view class="goodsNull">暂无商家订单</view></block></view>