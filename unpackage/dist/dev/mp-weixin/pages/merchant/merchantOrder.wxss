page {
  background-color: #f5f5f5;
}
.headerTabs {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
}
.headerTabs .tabs {
  width: 150rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  color: #333;
}
.headerTabs .activeTab {
  font-size: 36rpx;
  color: #FF2D2D;
}
.orderList {
  padding: 0 30rpx 30rpx;
}
.orderList .orderGoods {
  margin-bottom: 20rpx;
  width: 690rpx;
  background: #ffffff;
  border-radius: 20rpx;
  padding: 20rpx;
  font-size: 28rpx;
  color: #333;
}
.orderList .orderGoods .shopName {
  padding: 22rpx 20rpx;
  max-width: 400rpx;
}
.orderList .orderGoods .goods {
  display: flex;
  margin-bottom: 20rpx;
}
.orderList .orderGoods .goods .goodsImg {
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
  overflow: hidden;
  margin-right: 20rpx;
  flex-shrink: 0;
}
.orderList .orderGoods .goods .goodsInfo {
  width: 320rpx;
  flex-shrink: 0;
  margin-right: 12rpx;
}
.orderList .orderGoods .goods .goodsInfo .goodsName {
  height: 70rpx;
  margin-bottom: 10rpx;
}
.orderList .orderGoods .goods .goodsInfo .goodsSpecs {
  color: #999;
}
.orderList .orderGoods .goods .goodsPrice {
  font-size: 20rpx;
}
.orderList .orderGoods .distribution {
  margin-bottom: 30rpx;
}
.orderList .orderGoods .distribution .disMode {
  display: flex;
  max-height: 70rpx;
}
.orderList .orderGoods .distribution .disMode text {
  margin-left: 20rpx;
  color: #999999;
  max-width: 400rpx;
}
.orderList .orderGoods .distribution .disModePrice {
  color: #999;
}
.orderList .orderGoods .distribution:last-child {
  margin-bottom: 40rpx;
}
.orderList .orderGoods .priceDetail {
  width: 100%;
  text-align: right;
  display: flex;
  justify-content: flex-end;
  margin-bottom: 20rpx;
}
.orderList .orderGoods .priceDetail .totalPrice,
.orderList .orderGoods .priceDetail .discountPrice {
  color: #999;
}
.orderList .orderGoods .priceDetail .payPrice {
  margin-left: 12rpx;
}
.orderList .orderGoods .operation {
  width: 100%;
  text-align: right;
  display: flex;
  justify-content: flex-end;
}
.orderList .orderGoods .operation .operationBtn {
  padding: 10rpx 20rpx;
  border: 1rpx solid #cccccc;
  border-radius: 50rpx;
  margin-right: 30rpx;
}
.orderList .orderGoods .operation .operationBtn:last-child {
  margin-right: 0;
}
.orderList .orderGoods .operation .redBtn {
  color: #FF2D2D;
  border-color: #FF2D2D;
}
.yuan {
  font-size: 32rpx;
}
.jiao {
  font-size: 24rpx;
}
.mask {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 99;
}
.model {
  position: fixed;
  left: 50%;
  top: 40%;
  z-index: 999;
  -webkit-transform: translateX(-50%) translateY(-40%);
          transform: translateX(-50%) translateY(-40%);
  width: 488rpx;
  background: #ffffff;
  border-radius: 10rpx;
}
.model .title {
  font-size: 32rpx;
  color: #333;
  padding: 20rpx;
}
.model .content {
  margin-left: 20rpx;
  width: 448rpx;
  height: 56rpx;
  background: #f5f5f5;
  border-radius: 8rpx;
  margin-bottom: 35rpx;
  position: relative;
  font-size: 24rpx;
}
.model .content .selPicker {
  display: inline-block;
  height: 100%;
  width: 100%;
  padding: 16rpx 20rpx;
  box-sizing: border-box;
}
.model .content image {
  position: absolute;
  top: 16rpx;
  right: 20rpx;
  width: 24rpx;
  height: 24rpx;
}
.model .operation {
  height: 80rpx;
  width: 100%;
  border-top: 2rpx solid #EBEBEB;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.model .operation view {
  flex: 1;
  font-size: 32rpx;
  text-align: center;
}
.model .operation .cancel {
  color: #999999;
}
.model .operation .confirm {
  color: #FF2D2D;
}

